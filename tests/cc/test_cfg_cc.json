		"eqasm_backend_cc": {
			// Immutable properties of instruments. The keys "cc", "switch", "awg"
			// and "measure" are built-in
			"instrument-definitions": {
				"cc": [
					{	"name": "qutech-cc",
						"slots": 12
					}
				],
				"switch": [
					{	"name": "qutech-vsm",
						"channels": 32
					}
				],
				"awg": [
					{	"name": "qutech-qwg",
						"channels": 4,
						"codeword-groups": [4]
					},
					{	"name": "zi-hdawg",
						"channels": 8,
						"codeword-groups": [2, 4, 8]
					}
				],
				"measure": [
					{	"name": "zi-uhfqc",
						"channels": 9
					}
				]
			},

			// Instruments used in this setup, their configuration and connectivity.
			// The keys correspond to "name" fields defined in section "instrument-definitions"
			"instruments": {
				"qutech-cc": [
					{	"name": "cc",
						"ccio": [
							{	"slot": 0,
								"io_module": "cc-conn-dio-diff",
//								"driven-instrument": "awg8-mw-0",
								"code-word-mode": "cw8x3bits"
							},
							{	"slot": 1,
								"io_module": "cc-conn-dio-diff",
//								"driven-instrument": "awg8-flux-0",
								"code-word-mode": "cw8x3bits"
							},
							{	"slot": 2,
								"io_module": "cc-conn-dio",
//								"driven-instrument": "uhfqc-0",
								"code-word-mode": "mask10bits",
								"result": "mask10bits"
							},
							{	"slot": 3,
								"io_module": "cc-conn-vsm",
//								"driven-instrument": "vsm-0",
								"code-word-mode": "mask32bits"
							}
						]
					}
				],
				"zi-hdawg": [
					{	"name": "awg8-mw-0",
						"signal-type": "mw",
						"channels-per-group": 4,
						"next-instrument": "vsm-0"
						// FIXME":"channel assignment, which output to which VSM"
					},
					{	"name": "awg8-flux-0",
						"signal-type": "flux",
						"channels-per-group": 1,
						"qubits": [0, 1, 2, 3, 4, 5, 6, 7, 8]
					}
				],
				"zi-uhfqc": [
					{	"name": "uhfqc-0",
						"channels-per-group": 1,
						"qubits": [0, 1, 2, 3, 4, 5, 6, 7, 8]
					}
				],
				"qutech-vsm": [
					{	"name": "vsm-0",
						"channels-per-group": 1,
						"qubits": [0, 1, 2, 3, 4, 5, 6, 7, 8]
					}
				]
			},

